# Production Environment Variables for RBComputer Frontend
# These will be set in Netlify dashboard

# API Configuration - UPDATED WITH LIVE RENDER BACKEND
REACT_APP_API_BASE_URL=https://web-project-6plz.onrender.com/api
REACT_APP_API_TIMEOUT=30000

# Application Configuration
REACT_APP_APP_NAME=R.B Computer
REACT_APP_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true

# UI Configuration
REACT_APP_THEME=default
REACT_APP_LANGUAGE=en
REACT_APP_TIMEZONE=UTC

# External Services (if needed)
REACT_APP_SENTRY_DSN=
REACT_APP_GOOGLE_ANALYTICS_ID=

# Build Configuration
GENERATE_SOURCEMAP=true
INLINE_RUNTIME_CHUNK=false

# Disable console warnings in production
REACT_APP_DISABLE_CONSOLE_WARNINGS=true
