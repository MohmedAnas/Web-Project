# Development Environment Variables for RBComputer Frontend

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000/api
REACT_APP_API_TIMEOUT=10000

# Application Configuration
REACT_APP_APP_NAME=R.B Computer (Dev)
REACT_APP_APP_VERSION=1.0.0-dev
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_REPORTING=false
REACT_APP_ENABLE_PERFORMANCE_MONITORING=false

# UI Configuration
REACT_APP_THEME=default
REACT_APP_LANGUAGE=en
REACT_APP_TIMEZONE=UTC

# Development Tools
REACT_APP_ENABLE_REDUX_DEVTOOLS=true
REACT_APP_ENABLE_DEBUG_LOGS=true

# Build Configuration
GENERATE_SOURCEMAP=true
INLINE_RUNTIME_CHUNK=false
